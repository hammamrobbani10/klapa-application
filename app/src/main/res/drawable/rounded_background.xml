<!-- rounded_corners_background.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="-10dp" android:left="-10dp" android:right="-10dp" android:bottom="-10dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- Ganti dengan warna yang diinginkan -->
            <corners android:radius="100dp" /> <!-- Ganti dengan radius yang diinginkan -->
        </shape>
    </item>
</layer-list>
